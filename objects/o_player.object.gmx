            <kind>1</kind>
            <string>///Movement Engine
if (global.pause == 1) exit;
if (dead == 1) exit;

//Movement
if (keyboard_check(ord('W')) || (gamepad_axis_value(0,gp_axislv) &lt; 0) || gamepad_button_check(0,gp_padu)){
y -= global.spd;
}

if (keyboard_check(ord('S')) || (gamepad_axis_value(0,gp_axislv) &gt; 0) || gamepad_button_check(0,gp_padd)){
y += global.spd;
}

if (keyboard_check(ord('A')) || (gamepad_axis_value(0,gp_axislh) &lt; 0) || gamepad_button_check(0,gp_padl)){
x -= global.spd;
}

if (keyboard_check(ord('D')) || (gamepad_axis_value(0,gp_axislh) &gt; 0) || gamepad_button_check(0,gp_padr)){
x += global.spd;
}

image_speed = 0.3

//Looking
if(keyboard_check(vk_left) || (gamepad_axis_value(0,gp_axisrh) &lt; 0)) image_angle = image_angle + 4;
if(keyboard_check(vk_right) || (gamepad_axis_value(0,gp_axisrh) &gt; 0)) image_angle = image_angle - 4;
if((gamepad_axis_value(0,gp_axisrv) &lt; 0)) image_angle = image_angle + 4;
if((gamepad_axis_value(0,gp_axisrv) &gt; 0)) image_angle = image_angle - 4;



//Shooting
if(global.bullet123 = true &amp;&amp; (keyboard_check_pressed(vk_space) || gamepad_button_check_pressed(0, gp_shoulderrb)))
{
bullet = instance_create(x, y, o_bullet)

bullet.direction = image_angle  + 90;
bullet.image_angle = image_angle;
bullet.speed = 15;
}

//Lazer
var hasjuice = true;
if((keyboard_check(vk_space) || gamepad_button_check(0, gp_shoulderrb)) &amp;&amp; global.laserbeam123 == true &amp;&amp; laz = false)
    {
        if(global.juice &gt;= 0) 
        {
            global.juice = global.juice - 1;
        } 
        else
        {
            laz = true;
            alarm[2] = room_speed *3;
            global.juice = global.juice + 0;
        }
    }


    
    if(!keyboard_check(vk_space) &amp;&amp; global.laserbeam123 == true)
    {
        if(global.juice &lt;= 180) 
        {
            global.juice = global.juice + 1;
        } 
        else
        {
            global.juice = global.juice + 0;
        }
    }

if(laz = true)
{
   global.juice = 180;
}

//Manual Restart
if(keyboard_check_pressed(ord('R')))
{
    if(alarm[1] == -1)
    {
        rstart = true
        alarm[1] = room_speed *1.75;
    }
}

if(alarm[1] == 1 &amp;&amp; rstart == true)
{
    game_restart();
}
    



</string>
